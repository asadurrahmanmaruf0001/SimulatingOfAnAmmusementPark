<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="OOPProject.Akib.SparePartsOrderFxml">
   <children>
      <Label layoutX="200.0" layoutY="23.0" text="Spare Parts Order">
         <font>
            <Font name="System Bold" size="24.0" />

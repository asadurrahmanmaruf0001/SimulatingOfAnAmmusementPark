<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo2.LoginFxml">
   <children>
      <Label layoutX="263.0" layoutY="73.0" text="LOGIN" textFill="#892929">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="userIDTextField" layoutX="226.0" layoutY="140.0" promptText="User ID" />
      <PasswordField fx:id="passwordPasswordField" layoutX="227.0" layoutY="189.0" promptText="Password" />
      <Button layoutX="226.0" layoutY="266.0" mnemonicParsing="false" onAction="#continueButtonOnAction" prefHeight="26.0" prefWidth="150.0" text="Continue" />
      <CheckBox fx:id="showPasswordCheckBox" layoutX="269.0" layoutY="230.0" mnemonicParsing="false" prefHeight="18.0" prefWidth="107.0" text="Show Password" />
      <Label layoutX="238.0" layoutY="316.0" text="Don't have an account?" />
      <Label fx:id="showPasswordLabel" alignment="CENTER" layoutX="226.0" layoutY="344.0" prefHeight="18.0" prefWidth="150.0" />
      <Label alignment="CENTER" layoutY="-3.0" prefHeight="47.0" prefWidth="600.0" style="-fx-background-color: skyblue;" text="Fantasy Kingdom" textFill="#d10a0a">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Label alignment="TOP_LEFT" layoutX="428.0" layoutY="108.0" prefHeight="80.0" prefWidth="150.0" text="If ID 5 digit then manager, if ID 6 digits then security officer, if ID 7 digits then Maintenance Officer" wrapText="true" />
      <Label layoutX="428.0" layoutY="192.0" prefHeight="35.0" prefWidth="150.0" text="Password must be 8 characters" wrapText="true" />
      <Button layoutX="360.0" layoutY="312.0" mnemonicParsing="false" onAction="#createAccountButtonOnAction" style="-fx-background-color: transparent;" text="Create Account" textFill="#2709ea" underline="true">
         <font>
            <Font size="11.0" />
         </font></Button>
   </children>
</AnchorPane>
